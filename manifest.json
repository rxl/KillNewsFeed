{
  "name": "Feedblocker",
  "version": "2.0.0",
  "description": "Blocks feeds on Facebook and Twitter.",
  "content_scripts": [{
      "matches": [
        "*://*.facebook.com/*",
        "*://*.twitter.com/*"
      ],
      "js": [
        "jquery.min.js",
        "kill.js"
      ],
      "run_at": "document_start"
  }],
  "icons": {
    "16": "images/block-16.png",
    "48": "images/block-48.png",
    "128": "images/block-128.png"
  },
  "manifest_version": 2
}