{
  "name": "Feedblocker",
  "version": "2.1.0",
  "description": "Blocks feeds on various social sites, like Facebook and Twitter.",
  "content_scripts": [{
      "matches": [
        "*://*.facebook.com/*",
        "*://*.twitter.com/*",
        "*://news.ycombinator.com/*",
        "*://medium.com/*"
      ],
      "js": [
        "src/jquery.min.js",
        "src/facebook.js",
        "src/twitter.js",
        "src/hackernews.js",
        "src/medium.js",
      ],
      "run_at": "document_start"
  }],
  "icons": {
    "16": "images/block-16.png",
    "48": "images/block-48.png",
    "128": "images/block-128.png"
  },
  "manifest_version": 2
}